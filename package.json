  "scripts": {
    "dev": "node scripts/dev.js",
    "devServer": "webpack-cli serve",
    "css": "tsm 'src/style/**/*.module.scss' -n dashes -e default",
    "css:once": "yarn css",
    "css:watch": "yarn css -w",
